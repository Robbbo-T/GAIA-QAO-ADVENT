# GAIA-QAO-AdVent Example Environment Configuration

# Environment
GAIA_ENV=development
GAIA_DEBUG=true

# Aircraft Fleet
GAIA_AIRCRAFT_ID=AMPEL-360BWBH200-001
GAIA_FLEET_SIZE=10

# Quantum Providers
IBMQ_TOKEN=your-ibm-quantum-api-key
DWAVE_TOKEN=your-dwave-api-key
RIGETTI_TOKEN=your-rigetti-api-key
QUANTUM_BACKEND=simulator   # simulator|ibmq|dwave|rigetti
QUANTUM_SHOTS=1024
QPU_TIMEOUT=300

# Telemetry & Data
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=gaia-qao-telemetry
INFLUXDB_URL=http://localhost:8086
INFLUXDB_TOKEN=your-influxdb-token
INFLUXDB_ORG=gaia-qao
INFLUXDB_BUCKET=telemetry
TELEMETRY_RATE_HZ=100

# Dashboard/API
DASHBOARD_PORT=3000
API_GATEWAY_PORT=8080

# Security
JWT_SECRET=your-secure-jwt-secret
TLS_ENABLED=true
QUANTUM_ENCRYPTION=true

# Logging & Monitoring
GRAFANA_PORT=3001
PROMETHEUS_PORT=9090
JAEGER_PORT=16686
GAIA_LOG_LEVEL=INFO

# Miscellaneous
NODE_ENV=development
TIMEZONE=UTC

# Uncomment and set for external integrations
# SLACK_WEBHOOK_URL=
# SENTRY_DSN=
